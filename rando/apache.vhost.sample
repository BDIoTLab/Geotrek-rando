Alias /static/ /PATH_TO_SOURCE/media/static/

<Directory /PATH_TO_SOURCE/media/static>
  Order deny,allow
  Allow from all
</Directory>

Alias /media/ /PATH_TO_SOURCE/media/input/media/

<Directory /PATH_TO_SOURCE/media/input/media>
  Order deny,allow
  Allow from all
</Directory>

WSGIScriptAlias / /PATH_TO_SOURCE/rando/wsgi.py
WSGIPythonPath /PATH_TO_SOURCE/:/PATH_TO_SOURCE/lib/python2.7/site-packages
<Directory /PATH_TO_SOURCE/rando/>
    <Files wsgi.py>
        Order deny,allow
        Allow from all
    </Files>
</Directory>


